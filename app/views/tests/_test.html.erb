<tr>
  <td><%= test.title %></td>
  <td><%= test.category.title %></td>
  <td><%= test.level %></td>
  <td><%= test.author.present? ? test.author.name : '-' %></td>
  <td><%= test.questions.count %></td>
  <td><%= test.questions.count > 0 ? (button_to 'Start', start_test_path(test)) : '' %></td>
</tr>